		{
			'x1':'assets/images/b1.jpg',
			'x2':'assets/images/b1.jpg',
		},
		{
			'x1':'assets/images/b2.jpg',
			'x2':'assets/images/b1.jpg',
		},
		{
			'x1':'assets/images/b3.jpg',
			'x2':'assets/images/b3.jpg',
		},
		{
			'x1':'assets/images/b4.jpg',
			'x2':'assets/images/b4.jpg',
		},
		{
			'x1':'assets/images/b5.jpg',
			'x2':'assets/images/b5@2x.jpg',
		},
		{
			'x1':'assets/images/b6.jpg',
			'x2':'assets/images/b6.jpg',
